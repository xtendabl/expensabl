{
  "name": "expensabl",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "build": "npm run test && webpack --mode production",
    "build:dev": "NODE_ENV=development webpack --mode development",
    "watch": "NODE_ENV=development webpack --mode development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint:fix": "eslint src/**/*.ts --fix",
    "sort:imports": "eslint src/**/*.ts --fix",
    "format:fix": "eslint src/**/*.ts --fix && prettier --write \"src/**/*.{ts,js,json}\"",
    "prepare": "husky",
    "docs:build": "cd docs && mdbook build",
    "docs:serve": "cd docs && mdbook serve --open",
    "docs:watch": "cd docs && mdbook watch --open"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xtendabl/expensabl.git"
  },
  "private": true,
  "devDependencies": {
    "@types/chrome": "^0.0.251",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.9",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.2",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "langchain": "^0.3.30"
  }
}
